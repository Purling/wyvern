require stdout

import metadata tsls.formatstring

//need to deal with leading whitespace for parsing; maybe use ` to indicate
//beginning of format string or something?

def printf(str:formatstring.FormatString):Unit
    stdout.print(str)
    stdout.println()
    
//just a plain string
printf(~)
 Hello World!

printf(~)
 Hello %{"string!"}%{" {mm}"} and %{ "another string!" }
 
val x = "xvalue"
printf(~)
 %{x}