import fileSystem.fileEffects
resource type RandomAccessFile //can be renamed later if necessary
    def close() : {fileEffects.close} Unit
    def getPosition() : {fileEffects.read}  Int
    def seek(pos : Int) : {fileEffects.read} Unit
    def length() : {fileEffects.read} Int
    def readLine() : {fileEffects.read} String
    def readUTF() : {fileEffects.read} String
    def write(s : String) : {fileEffects.write} Unit
    def writeUTF(s : String) : {fileEffects.write} Unit